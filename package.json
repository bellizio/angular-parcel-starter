{
  "name": "angular-parcel-starter",
  "version": "1.0.0",
  "description": "An Angular starter kit with Parcel",
  "repository": "https://github.com/bellizio/angular-parcel-starter.git",
  "author": "Devon Bellizio",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rm -rf dist && rm -rf .cache",
    "prestart": "npm run clean",
    "prebuild": "npm run clean",
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html --public-url ./"
  },
  "dependencies": {
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "core-js": "^2.5.7",
    "rxjs": "^5.5.7",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@types/node": "^9.4.7",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.5.1",
    "parcel-plugin-angular": "^0.5.0",
    "typescript": "~2.5.2"
  }
}
